<Defs>



<RecipeDef ParentName="GrenadeRecipeBase">
		<defName>MakeTrapExplosive</defName>
		<label>Создание коробки 6 мин</label>
		<description>Создание колобки с 6 минами.</description>
		<jobString>Создаёт набор мин.</jobString>
		<workAmount>1000</workAmount>
		<targetCountAdjustment>10</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Wire</li>
					</thingDefs>
				</filter>
				<count>12</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
						<li>FSX</li>
					</thingDefs>
				</filter>
				<count>12</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Mechanism</li>
					</thingDefs>
				</filter>
				<count>6</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
			</categories>
			<thingDefs>
				<li>Powder</li>
				<li>FSX</li>
				<li>Wire</li>
				<li>Mechanism</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Trap2_Explosive>6</Trap2_Explosive>    
		</products>
	</RecipeDef>
	
	
